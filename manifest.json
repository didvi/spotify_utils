{
    "manifest_version": 2,
    "name": "Spotify Utils",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://open.spotify.com/*"
          ],
          "js": ["content.js"]
        }
    ],
    "permissions": [
        "https://api.spotify.com/v1"
    ],
    "browser_action": {
        "default_icon": "spotify.png"
    }, 
    "background": {
        "scripts": ["background.js"]
    }
  }